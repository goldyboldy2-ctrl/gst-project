<script>
// State code mapping
const stateCodes = {
  '01': 'Jammu & Kashmir', '02': 'Himachal Pradesh', '03': 'Punjab', '04': 'Chandigarh',
  '05': 'Uttarakhand', '06': 'Haryana', '07': 'Delhi', '08': 'Rajasthan',
  '09': 'Uttar Pradesh', '10': 'Bihar', '11': 'Sikkim', '12': 'Arunachal Pradesh',
  '13': 'Nagaland', '14': 'Manipur', '15': 'Mizoram', '16': 'Tripura',
  '17': 'Meghalaya', '18': 'Assam', '19': 'West Bengal', '20': 'Jharkhand',
  '21': 'Odisha', '22': 'Chhattisgarh', '23': 'Madhya Pradesh', '24': 'Gujarat',
  '26': 'Dadra & Nagar Haveli and Daman & Diu', '27': 'Maharashtra',
  '29': 'Karnataka', '30': 'Goa', '31': 'Lakshadweep', '32': 'Kerala',
  '33': 'Tamil Nadu', '34': 'Puducherry', '35': 'Andaman & Nicobar',
  '36': 'Telangana', '37': 'Andhra Pradesh', '38': 'Ladakh'
};

function verifyGSTIN() {
  const gstin = document.getElementById('gstin').value.toUpperCase().trim();
  const resultBox = document.getElementById('resultBox');

  // Basic validation
  if (!gstin) {
    alert('Please enter a GSTIN');
    return;
  }

  if (gstin.length !== 15) {
    resultBox.className = 'result-box danger';
    resultBox.innerHTML = `
      <div class="result-icon">‚ùå</div>
      <h3>Invalid GSTIN Format</h3>
      <p>GSTIN must be exactly <strong>15 characters</strong>. You entered ${gstin.length} characters.</p>
      <p style="margin-top:12px; font-size:0.9rem;">Correct format: 2 digits + 10 alphanumeric (PAN) + 1 digit + 1 letter + 1 digit</p>
    `;
    resultBox.style.display = 'block';
    return;
  }

  // Extract components
  const stateCode = gstin.substring(0, 2);
  const pan = gstin.substring(2, 12);
  const entityNumber = gstin.substring(12, 13);
  const defaultChar = gstin.substring(13, 14);
  const checkDigit = gstin.substring(14, 15);

  const stateName = stateCodes[stateCode] || 'Unknown State';

  // Validate format
  const stateCodeValid = /^\d{2}$/.test(stateCode);
  const panValid = /^[A-Z]{5}\d{4}[A-Z]$/.test(pan);
  const entityValid = /^\d$/.test(entityNumber);
  const defaultValid = /^[A-Z]$/.test(defaultChar);
  const checkValid = /^[A-Z0-9]$/.test(checkDigit);

  if (!stateCodeValid || !panValid || !entityValid || !defaultValid || !checkValid) {
    resultBox.className = 'result-box danger';
    resultBox.innerHTML = `
      <div class="result-icon">‚ùå</div>
      <h3>Invalid GSTIN Format</h3>
      <p>The GSTIN you entered does not match the standard format.</p>
      <div style="background:white; border-radius:8px; padding:16px; margin-top:16px;">
        <p style="margin-bottom:8px;"><strong>Format Requirements:</strong></p>
        <ul style="margin:0; padding-left:20px; color:var(--text-sm);">
          <li>Positions 1-2: 2 digits (State Code) ${stateCodeValid ? '‚úÖ' : '‚ùå'}</li>
          <li>Positions 3-12: Valid PAN format ${panValid ? '‚úÖ' : '‚ùå'}</li>
          <li>Position 13: 1 digit (Entity number) ${entityValid ? '‚úÖ' : '‚ùå'}</li>
          <li>Position 14: 1 letter ${defaultValid ? '‚úÖ' : '‚ùå'}</li>
          <li>Position 15: 1 alphanumeric (Check digit) ${checkValid ? '‚úÖ' : '‚ùå'}</li>
        </ul>
      </div>
    `;
    resultBox.style.display = 'block';
    return;
  }

  // Simulated verification (in real app, you'd call GST API)
  // For demo purposes, we'll show format validation success
  resultBox.className = 'result-box safe';
  resultBox.innerHTML = `
    <div class="result-icon">‚úÖ</div>
    <h3>GSTIN Format Valid</h3>
    <div style="background:white; border-radius:8px; padding:24px; margin-top:20px;">
      <table style="width:100%; border-collapse:collapse;">
        <tr style="border-bottom:2px solid var(--border);">
          <td style="padding:12px 0; font-weight:600;">GSTIN:</td>
          <td style="padding:12px 0; text-align:right; font-family:monospace; font-size:1.1rem; color:var(--navy);">${gstin}</td>
        </tr>
        <tr style="border-bottom:1px solid var(--border);">
          <td style="padding:12px 0; font-weight:600;">State:</td>
          <td style="padding:12px 0; text-align:right;">${stateName} (${stateCode})</td>
        </tr>
        <tr style="border-bottom:1px solid var(--border);">
          <td style="padding:12px 0; font-weight:600;">PAN:</td>
          <td style="padding:12px 0; text-align:right; font-family:monospace;">${pan}</td>
        </tr>
        <tr style="border-bottom:1px solid var(--border);">
          <td style="padding:12px 0; font-weight:600;">Entity Number:</td>
          <td style="padding:12px 0; text-align:right;">${entityNumber}</td>
        </tr>
        <tr style="border-bottom:1px solid var(--border);">
          <td style="padding:12px 0; font-weight:600;">Format Status:</td>
          <td style="padding:12px 0; text-align:right; color:var(--green); font-weight:600;">‚úÖ Valid Format</td>
        </tr>
      </table>
    </div>

    <div style="background:#e0f2fe; border:1px solid#0ea5e9; border-radius:8px; padding:16px; margin-top:20px;">
      <h4 style="margin:0 0 8px; color:#0c4a6e;">‚ÑπÔ∏è Important Note:</h4>
      <p style="margin:0; color:#0c4a6e; font-size:0.9rem;">
        This tool verifies the GSTIN <strong>format</strong> only. To check actual registration status (Active/Cancelled), business name, and other details, visit the official GST portal at <a href="https://services.gst.gov.in/services/searchtp" target="_blank" style="color:#0284c7; text-decoration:underline;">services.gst.gov.in/services/searchtp</a>
      </p>
    </div>
  `;

  resultBox.style.display = 'block';
  resultBox.scrollIntoView({behavior:'smooth', block:'center'});
}

// Auto-capitalize input
document.getElementById('gstin').addEventListener('input', (e) => {
  e.target.value = e.target.value.toUpperCase();
});

// Enter key support
document.getElementById('gstin').addEventListener('keypress', (e) => {
  if (e.key === 'Enter') verifyGSTIN();
});
</script>

